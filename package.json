{
  "name": "ansi-code",
  "version": "1.0.0",
  "description": "Catalog of ANSI standard codes for ASCII terminals",
  "main": "lib/common/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib",
    "src",
    "!lib/**/*.tsbuildinfo",
    "tsconfig.json",
    "tsconfig.esm.json"
  ],
  "scripts": {
